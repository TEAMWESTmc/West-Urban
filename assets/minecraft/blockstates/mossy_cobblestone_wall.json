{
    "multipart": [
        {   "when": { "north": "none", "south": "none", "east": "none", "west": "none"},
            "apply": { "model": "block/mossy_cobblestone_wall_single"}
        },
        {   "when": { "OR": [{"up": true, "north": "tall"},
                            {"up": true, "south": "tall"},
                            {"up": true, "east": "tall"},
                            {"up": true, "west": "tall"}]},
            "apply": { "model": "block/mossy_cobblestone_wall_post" }
        },
        {   "when": { "OR": [{"north": "tall"},
                            {"south": "tall"}]},
            "apply": { "model": "block/mossy_cobblestone_wall_filler" }
        },
        {   "when": { "OR": [{"west": "tall"},
                            {"east": "tall"}]},
            "apply": { "model": "block/mossy_cobblestone_wall_filler", "y": 90}
        },
        {   "when": { "north": "tall" },
            "apply": { "model": "block/mossy_cobblestone_wall_side", "uvlock": false}
        },
        {   "when": { "east": "tall" },
            "apply": { "model": "block/mossy_cobblestone_wall_side", "y": 90 }
        },
        {   "when": { "south": "tall" },
            "apply": { "model": "block/mossy_cobblestone_wall_side", "y": 180 }
        },
        {   "when": { "west": "tall" },
            "apply": { "model": "block/mossy_cobblestone_wall_side", "y": 270 }
        },{   "when": { "north": "none", "south": "none", "east": "none", "west": "none"},
            "apply": { "model": "block/mossy_cobblestone_wall_single"}
        },
        {   "when": { "OR": [{"up": true, "north": "low"},
                            {"up": true, "south": "low"},
                            {"up": true, "east": "low"},
                            {"up": true, "west": "low"}]},
            "apply": { "model": "block/mossy_cobblestone_wall_post" }
        },
        {   "when": { "OR": [{"north": "low"},
                            {"south": "low"}]},
            "apply": { "model": "block/mossy_cobblestone_wall_filler" }
        },
        {   "when": { "OR": [{"west": "low"},
                            {"east": "low"}]},
            "apply": { "model": "block/mossy_cobblestone_wall_filler", "y": 90}
        },
        {   "when": { "north": "low" },
            "apply": { "model": "block/mossy_cobblestone_wall_side", "uvlock": false}
        },
        {   "when": { "east": "low" },
            "apply": { "model": "block/mossy_cobblestone_wall_side", "y": 90 }
        },
        {   "when": { "south": "low" },
            "apply": { "model": "block/mossy_cobblestone_wall_side", "y": 180 }
        },
        {   "when": { "west": "low" },
            "apply": { "model": "block/mossy_cobblestone_wall_side", "y": 270 }
        }

    ]
}
